{
    "name": "davibennun/laravel-push-notification",
    "description": "Laravel package to send push notifications to mobile devices (apns, gcm)",
     "keywords": ["apns","gcm","push","notification", "laravel"],
    "authors": [
        {
            "name": "DaviBenNun",
            "email": "davi@andradenunes.org"
        }
    ],
    "repositories": {
        "notification-pusher": {
            "type": "package",
            "package": {
                "name": "sly/notification-pusher",
                "description": "Sly notification pusher with some fixes",
                "version": "2.0",
                "source": {
                    "url": "https://github.com/ndougoud/NotificationPusher",
                    "type": "git",
                    "reference": "852ba0014c6aefeecd7db4357bfd5e3e37713dc0"
                }
            }
        }
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~4.0",
        "sly/notification-pusher": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Davibennun\\LaravelPushNotification": "src/"
        }
    },
    "minimum-stability": "dev"
}
